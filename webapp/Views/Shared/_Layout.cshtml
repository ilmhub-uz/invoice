<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet"
        href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    <link rel="stylesheet" href="plugins/fontawesome-free/css/all.min.css">
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <link rel="stylesheet" href="dist/css/adminlte.min.css">
</head>

<body class="hold-transition sidebar-mini">
    <header>
        <div class="wrapper">

            <aside class="main-sidebar sidebar-dark-primary elevation-4 position-fixed">
                <a href="/" class="brand-link">
                    <img src="dist/images.jpg" class="brand-image img-circle elevation-3" style="opacity: .8">
                    <span class="brand-text font-weight-light">Invoice</span>
                </a>
                <div class="sidebar mt-3">
                    <nav class="mt-2">
                        <ul class="nav nav-pills nav-sidebar flex-column" role="menu" data-accordion="false">

                            <li class="nav-item menu-open">
                                <a href="/">
                                    <i class="fas fa-file-invoice fa-lg ml-4 mt-2"></i>
                                      <button type="button" class="btn btn-outline-secondary" style="border:none;
                                             height:40px;
                                             width:185px;
                                            "><h5 class="list-inline-item mr-5">Invoicelar</h5></button>
                                </a>
                                <ul class="nav nav-treeview">
                                </ul>
                            </li>

                        </ul>
                    </nav>
                </div>

                <div class="sidebar">
                    <nav class="mt-2">
                        <ul class="nav nav-pills nav-sidebar flex-column" role="menu" data-accordion="false">

                            <li class="nav-item menu-open">
                                <a href="/">
                                    <i class="fas fa-warehouse ml-4 mt-2"></i>
                                      <button type="button" class="btn btn-outline-secondary" style="border:none;
                                             height:40px;
                                             width:185px;
                                            "><h5 class="list-inline-item mr-5">Korxonalar</h5></button>
                                </a>
                                <ul class="nav nav-treeview">
                                </ul>
                            </li>

                        </ul>
                    </nav>
                </div>

                <div class="sidebar">
                    <nav class="mt-2">
                        <ul class="nav nav-pills nav-sidebar flex-column" role="menu" data-accordion="false">

                            <li class="nav-item menu-open">
                                <a href="/">
                                    <i class="fas fa-id-card-alt ml-4 mt-2"></i>
                                   <button type="button" class="btn btn-outline-secondary" style="border:none;
                                             height:40px;
                                             width:185px;
                                            "><h5 class="list-inline-item mr-5">Kantaktlar</h5></button>
                                </a>
                                <ul class="nav nav-treeview">
                                </ul>
                            </li>

                        </ul>
                    </nav>
                </div>

                <div class="sidebar">
                    <nav class="mt-2">
                        <ul class="nav nav-pills nav-sidebar flex-column" role="menu" data-accordion="false">

                            <li class="nav-item menu-open">
                                <a href="/">
                                    <i class="fas fa-chart-pie ml-4 mt-2"></i>
                                    <button type="button" class="btn btn-outline-secondary" style="border:none;
                                             height:40px;
                                             width:185px;
                                            "><h5 class="list-inline-item mr-5">Statistika</h5></button>
                                </a>
                                <ul class="nav nav-treeview">
                                </ul>
                            </li>

                        </ul>
                    </nav>
                </div>
            </aside>
        </div>
    </header>

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <script src="plugins/jquery/jquery.min.js"></script>
    <script src="plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="dist/js/adminlte.js"></script>

    <script src="dist/js/demo.js"></script>
    <script src="dist/js/pages/dashboard3.js"></script>
</body>

</html>
